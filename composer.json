{
    "name": "techdock/opcua-webapi-client",
    "description": "PHP client for OPC UA Web API",
    "keywords": [
        "opcua",
        "webapi",
        "client",
        "php"
    ],
    "homepage": "https://github.com/TECHDOCK-CH/opcua-webapi-client",
    "license": "MIT",
    "authors": [
        {
            "name": "TechDock",
            "email": "info@techdock.ch"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^3.5"
    },
    "autoload": {
        "psr-4": { "TechDock\\OpcUaWebApiClient\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "TechDock\\OpcUaWebApiClient\\Test\\" : "test/" }
    },
    "scripts": {
        "test": "phpunit",
        "test:live": "ENABLE_LIVE_TESTS=1 phpunit --verbose"
    }
}
